{
  "manifest_version":2,

  "name":"PGA Leaderboard",
  "short_name":"ScoreBoard",
  "version":"0.1",

  "permissions":[
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.pgatour.com/*"
  ],

  "homepage_url":"http://insert_web_site_here.com/",
  "browser_action":{
    "default_title":"Pga Leaderboard",
    
    "default_popup":"popup.html"
  },
  "background":{
    "scripts":["scripts/jquery.js","scripts/background.js"],
    "presistent":false
  }
}
